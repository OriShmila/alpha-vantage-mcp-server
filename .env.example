# Alpha Vantage MCP Server Configuration
# Copy this file to .env and fill in your actual API key

# Alpha Vantage API Key (Required)
# Get your free API key at: https://www.alphavantage.co/support/#api-key
ALPHA_VANTAGE_KEY=your_alpha_vantage_api_key_here

# Optional: Debug mode
# DEBUG=false

# Optional: Rate limiting settings
# API_RATE_LIMIT=5  # requests per minute for free tier
# API_RATE_LIMIT=75  # requests per minute for premium tier

# Note: Free tier allows 25 requests per day and 5 API requests per minute
# Premium tier starts at $49.99/month with 75 API requests per minute
